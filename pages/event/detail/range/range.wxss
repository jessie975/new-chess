@import '/style/main.wxss';

.container {
    width: 90%;
    margin: 10rpx auto;
}
.scroll-view {
    height: 100%;
}
.range-list .range {
    width: 50rpx;
    height: 50rpx;
}
.range-list .range .rang-icon {
    width: 100%;
    height: 100%;
}
.title-container {
    width: 100%;
}
.nav {
	white-space: nowrap;
}

::-webkit-scrollbar {
	display: none;
}

.nav .cu-item {
	display: inline-block;
	margin: 0 10rpx;
	padding: 0 30rpx;
}

.nav .cu-item.cur {
	border-bottom: 4rpx solid;
}

.column-item {
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
}