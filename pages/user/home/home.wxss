@import '/style/main.wxss';
@import '/style/iconfont.wxss';
.user-info,
.number-area,
.entry {
    width: 90%;
    margin: 30rpx auto;
    max-height: 300px;
    overflow: scroll;
}
.user-info {
    margin-left: 100rpx;
    margin-top: 60rpx;
}
.avatar {
    width: 140rpx;
    height: 140rpx;
    border-radius: 50%;
    overflow: hidden;
}
.avatar .image {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}