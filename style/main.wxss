page {
    --white: #FAF5EF;
    --black: #423928;
    --theme: #CDB68A;
    --text: #553E38;
    --lightText: #756B60;
    --darkText: #84775A;
    --green: #497749;
    --bg: #D4CAA8;

    font-size: 28rpx;
    font-family: Helvetica Neue, Helvetica, sans-serif;
}

view,
scroll-view,
swiper,
button,
input,
textarea,
label,
navigator,
image {
    box-sizing: border-box;
}

.bg-img {
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: -999;
}

.border {
    border: 1px solid #CDB68A;
}

.border-bottom {
    border-bottom: 1px solid #D4CAA8;
}

.radius {
    border-radius: 10rpx;
}

.bg-color {
    background: radial-gradient(#D4CAA8, #CDB68A);
}

/* ==================
        布局
 ==================== */
.flex {
    display: flex;
}

.justify-center {
    justify-content: center;
}

.justify-between {
    justify-content: space-between;
}

.justify-around {
    justify-content: space-around;
}

.justify-end {
    justify-content: flex-end;
}

.direction-column {
    flex-direction: column;
}

.align-center {
    align-items: center;
}

.flex-wrap {
    flex-wrap: wrap;
}

/* ==================
        文本
 ==================== */

.text-sm {
    font-size: 22rpx;
}

.text-df {
    font-size: 24rpx;
}

.text-lg {
    font-size: 32rpx;
}

.text-xl {
    font-size: 36rpx;
}

.text-xxl {
    font-size: 44rpx;
}

.text-cut {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}

.text-bold {
    font-weight: bold;
}

.text-theme {
    color: var(--theme);
}

.text-black {
    color: var(--black);
}

.text-white {
    color: var(--white);
}

.text-text {
    color: var(--text);
}

.text-lightText {
    color: var(--lightText)
}

.text-darkText {
    color: var(--darkText);
}

.text-green {
    color: var(--green);
}

.text-center {
    text-align: center;
}

/* ==================
        轮播
 ==================== */

.screen-swiper {
    min-height: 375rpx;
}

.screen-swiper image,
.swiper-item image {
    width: 100%;
    display: block;
    height: 100%;
    margin: 0;
    pointer-events: none;
}

.card-swiper {
    height: 950rpx !important;
}

.card-swiper swiper-item {
    width: 580rpx !important;
    left: 70rpx;
    box-sizing: border-box;
    padding: 40rpx 0rpx 70rpx;
    overflow: initial;
}

.card-swiper swiper-item .swiper-item {
    width: 100%;
    display: block;
    height: 100%;
    border-radius: 10rpx;
    transform: scale(0.9);
    transition: all 0.2s ease-in 0s;
    overflow: hidden;
}

.card-swiper swiper-item.cur .swiper-item {
    transform: none;
    transition: all 0.2s ease-in 0s;
}

