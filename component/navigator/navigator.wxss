} */
.container .item {
    font-size: 30rpx;
    height: 65rpx;
    line-height: 65rpx;
    flex: 1;
    text-align: center;
    position: relative;
}
.container .item .move-bg {
    width: 90%;
    height: 80%;
    position: absolute;
    left: 5%;
    top: 10%;
    z-index: -10;
}
.text-text {
    color: #CDB68A;
}
.text-darkText {
    color: #84775A;
}