.container {
    height: 65rpx;
    position: relative;
    display: flex;
    justify-content: space-around;
    align-items: center;
}
.container .bg-img {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: -99;
}
